{
  "name": "nikola-generator",
  "version": "1.0.6",
  "description": "Quickly create and opinionated .eslintrc.js with built-in prettier rules ",
  "publishConfig": {
    "access": "public"
  },
  "main": "dist/nikola.js",
  "bin": {
    "nikola": "./dist/nikola.js"
  },
  "scripts": {
    "start": "node src/nikola.js",
    "start:dev": "nodemon src/nikola.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/NikolaM-Dev/nikola-generator.git"
  },
  "keywords": [
    "eslint",
    ".eslintrc.json",
    "prettier",
    "node",
    "typescript",
    "react",
    "next",
    "nest",
    "javascript",
    "js",
    "ts"
  ],
  "author": "David Merchan",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/NikolaM-Dev/nikola-generator/issues"
  },
  "homepage": "https://github.com/NikolaM-Dev/nikola-generator#readme",
  "devDependencies": {
    "@babel/core": "^7.16.0",
    "eslint": "^8.3.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-prettier": "^4.0.0",
    "nodemon": "^2.0.15",
    "prettier": "^2.4.1"
  },
  "dependencies": {
    "inquirer": "^8.2.0"
  }
}
